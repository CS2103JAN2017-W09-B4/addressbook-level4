<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@DarkTheme.css" />
      <URL value="@Extensions.css" />
   </stylesheets>
   <children>
      <MenuBar prefHeight="38.0" prefWidth="2895.0" VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Help" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane fx:id="commandBoxPlaceholder" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </AnchorPane>
       <AnchorPane fx:id="resultDisplayPlaceholder" maxHeight="100" minHeight="100" prefHeight="100" prefWidth="2852.0" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
           <padding>
               <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
           </padding>
       </AnchorPane>
      <TabPane prefWidth="2722.0" tabMinHeight="50.0" tabMinWidth="200.0" VBox.vgrow="ALWAYS">
         <tabs>
            <Tab closable="false" text="Priority ">
               <content>
                  <SplitPane id="splitPane" fx:id="splitPane" dividerPositions="0.5">
                     <items>
                        <VBox fx:id="personList" minWidth="340" prefWidth="340">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                           <children>
                              <AnchorPane fx:id="taskListPanelPlaceholder" prefHeight="150.0" prefWidth="1199.0" VBox.vgrow="ALWAYS" />
                           </children>
                        </VBox>
                        <AnchorPane fx:id="completedTaskListPlaceHolder" prefWidth="340">
                            <padding>
                                <Insets bottom="10" left="10" right="10" top="10" />
                            </padding>
                        </AnchorPane>
                     </items>
                  </SplitPane>
               </content>
            </Tab>
            <Tab closable="false" text="Overview">
               <content>
                  <SplitPane id="splitPane" fx:id="splitPane1" dividerPositions="0.5">
                     <items>
                        <VBox fx:id="personList1" minWidth="340" prefWidth="340">
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                           <children>
                              <AnchorPane fx:id="taskListPanelPlaceholder1" prefHeight="150.0" prefWidth="1199.0" VBox.vgrow="ALWAYS" />
                           </children>
                        </VBox>
                        <AnchorPane fx:id="browserPlaceholder1" prefWidth="340">
                           <padding>
                              <Insets bottom="10" left="10" right="10" top="10" />
                           </padding>
                        </AnchorPane>
                     </items>
                  </SplitPane>
               </content>
            </Tab>
            <Tab closable="false" text="Today">
               <content>
                  <SplitPane id="splitPane" fx:id="splitPane2" dividerPositions="0.5">
                     <items>
                        <VBox fx:id="personList2" minWidth="340" prefWidth="340">
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                           <children>
                              <AnchorPane fx:id="taskListPanelPlaceholder2" prefHeight="150.0" prefWidth="1199.0" VBox.vgrow="ALWAYS" />
                           </children>
                        </VBox>
                        <AnchorPane fx:id="browserPlaceholder2" prefWidth="340">
                           <padding>
                              <Insets bottom="10" left="10" right="10" top="10" />
                           </padding>
                        </AnchorPane>
                     </items>
                  </SplitPane>
               </content>
            </Tab>
         </tabs>
         <VBox.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </VBox.margin>
      </TabPane>
      <AnchorPane fx:id="statusbarPlaceholder" VBox.vgrow="NEVER" />
   </children>
</VBox>
